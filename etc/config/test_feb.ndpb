# Initialize current board
setrocdef 
# Select the FEB on First connector
feb 0
# Reset the nXYTER
resetnxi2cbus
resetnxi2creg
# Initialize current FEB
sfd

# Print current status
printstatus

# enable nxyter 0 data readout
setnxactive 1,0

# Search for the right VbiasS setting for current FEB
autovbiass

# change mode to enable pulser trigger in current nXYTER
sxmo ttri=1

# Change IO mode to nXYTER + CDAQ
ndpbio 0,1
