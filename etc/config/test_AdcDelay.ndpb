# Connect a board and print its status
//b=ndpbC.udp.0?etc/ipbus_remchub_ndpb.xml

feb 0
setrocdef

mucswrst

resetnxi2cbus
resetnxi2creg
sfd
printstatus

# change mode to disable pulser trigger in current nXYTER
sxmo ttri=0

# Read threshold value and then change it
getnx 18
setnx 18 20

# enable nxyter 0 data readout
setnxactive 1,0

# Change the ADC clock delay to one known to work with the 160MHz firmware
setadcpatt tm0=4,tm1=4,tm2=4,tm3=4
setadcclock 0
getadcdirect
getadcdirect
